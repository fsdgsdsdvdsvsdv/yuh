        dbgWarn(VcSVtsDCpzFWSfXCIVCaOSErQFNv({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,12180,11655,10815,10815,11340,10605,10500,6090,3360}) .. tostring(state))
