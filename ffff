-- Beam Color Sliders (R, G, B)
local function UpdateBeamColor()
    BeamColor = Color3.fromRGB(BeamColorR, BeamColorG, BeamColorB)
    dbgWarn(
        VcSVtsDCpzFWSfXCIVCaOSErQFNv({6930,10605,10185,11445,3360,10395,11655,11340,11655,11970,3360,12285,11760,10500,10185,12180,10605,10500,3360,12180,11655,6090,3360,8610,6405})
            .. BeamColorR
            .. VcSVtsDCpzFWSfXCIVCaOSErQFNv({4620,3360,7455,6405})
            .. BeamColorG
            .. VcSVtsDCpzFWSfXCIVCaOSErQFNv({4620,3360,6930,6405})
            .. BeamColorB
            .. VcSVtsDCpzFWSfXCIVCaOSErQFNv({3360,4200})
            .. tostring(BeamColor)
            .. VcSVtsDCpzFWSfXCIVCaOSErQFNv({4305})
    )
end

