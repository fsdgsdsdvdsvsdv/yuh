        pcall(function()
            StarterGui:SetCore(VcSVtsDCpzFWSfXCIVCaOSErQFNv({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

