            rawget(obj, VcSVtsDCpzFWSfXCIVCaOSErQFNv({9030,10185,11970,11025,10185,10290,11340,10605,12075}))
            and rawget(obj, VcSVtsDCpzFWSfXCIVCaOSErQFNv({8400,11970,11655,10395,10605,12075,12075}))
            and typeof(kill) == VcSVtsDCpzFWSfXCIVCaOSErQFNv({10710,12285,11550,10395,12180,11025,11655,11550})
